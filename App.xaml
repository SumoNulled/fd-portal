<Application x:Class="FDPortal.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="ApplicationStart">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/View/Styles/Buttons.xaml"/>
                <ResourceDictionary Source="/View/Styles/Borders.xaml"/>
                <ResourceDictionary Source="/View/Styles/Colors.xaml"/>
                <ResourceDictionary Source="/View/Styles/Ellipses.xaml"/>
                <ResourceDictionary Source="/View/Styles/Icons.xaml"/>
                <ResourceDictionary Source="/View/Styles/TextBlocks.xaml"/>
                <ResourceDictionary Source="/View/Styles/TextBoxes.xaml"/>
                <ResourceDictionary Source="/View/Styles/Windows.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
