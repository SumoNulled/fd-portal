<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Style Colors -->
    <SolidColorBrush x:Key="PrimaryColor" Color="#151f2d"/>
    <SolidColorBrush x:Key="SecondaryColor" Color="#1e2e44"/>
    
    <SolidColorBrush x:Key="AlternatePrimaryColor" Color="#80151f2d"/>


    <SolidColorBrush x:Key="AlternateSecondaryColor" Color="#303e52"/>
    
    
    <SolidColorBrush x:Key="PrimaryAccent" Color="#7166f9"/>
    <SolidColorBrush x:Key="PrimaryAccentHover" Color="#938bfc"/>
    <SolidColorBrush x:Key="SecondaryAccent" Color="#b23157"/>

    <SolidColorBrush x:Key="PrimaryError" Color="#fa6b6b"/>
    
    <SolidColorBrush x:Key="PrimaryColorHover" Color="#7368ff"/>

    <SolidColorBrush x:Key="PrimaryForeground" Color="#edeeee"/>

    <Color x:Key="PrimaryForegroundHover">#7368ff</Color>
    
    <Color x:Key="GradientPrimary">#151f2d</Color>
    <Color x:Key="GradientSecondary">#364c6b</Color>

    <Color x:Key="GradientAlternatePrimary">#3b4b63</Color>
    <Color x:Key="GradientAlternateSecondary">#303e52</Color>




</ResourceDictionary>